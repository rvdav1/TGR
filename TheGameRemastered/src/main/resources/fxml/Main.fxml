<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="480" maxWidth="640" minHeight="480" minWidth="640" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MainController">
   <children>
      <Button fx:id="mainPlayButton" alignment="CENTER" mnemonicParsing="false" onAction="#handleMainPlay" prefHeight="60.0" text="Play" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="120.0" AnchorPane.topAnchor="40.0">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="mainOptionsButton" mnemonicParsing="false" onAction="#handleMainOptions" prefHeight="60.0" text="Options" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="120.0" AnchorPane.topAnchor="180.0">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="mainExitButton" mnemonicParsing="false" onAction="#handleMainExit" prefHeight="60.0" text="Exit" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="120.0" AnchorPane.topAnchor="320.0">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
